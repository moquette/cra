{
  "name": "create-react-cra",
  "version": "0.1.2",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn pre-build && react-scripts build",
    "lint": "npx prettier './{src,webpack}/**/*.{js,jsx,ts,tsx,json,scss,css,md,html}' --write && yarn tsc --noEmit && npx eslint './src/**/*.{ts,tsx}' --fix",
    "postinstall": "husky install"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.40",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "autoprefixer": "^10.4.7",
    "git-branch-is": "^4.0.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "sass": "^1.52.2",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.7.3"
  },
  "packageManager": "yarn@3.2.0"
}
